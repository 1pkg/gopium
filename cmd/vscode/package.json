{
	"name": "gopium",
	"displayName": "gopium",
	"description": "",
	"version": "0.0.1",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/*.go",
		"onLanguage:go",
		"onCommand:go.gopath",
		"onCommand:go.tools.install",
		"onDebugResolve:go"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gopium",
				"title": "Gopium cli runner"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./"
	},
	"extensionDependencies": [],
	"dependencies": {
		"diff": "^4.0.2",
		"json-rpc2": "^1.0.2",
		"moment": "^2.24.0",
		"semver": "^7.3.2",
		"tree-kill": "^1.2.2",
		"vscode-debugadapter": "^1.40.0",
		"vscode-debugprotocol": "^1.40.0",
		"vscode-extension-telemetry": "^0.1.2",
		"vscode-languageclient": "6.1.0",
		"web-request": "^1.0.7",
		"deep-equal": "^2.0.2"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.1.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.1",
		"@types/semver": "^7.1.0",
		"@types/sinon": "^9.0.0",
		"@types/vscode": "^1.41.0",
		"fs-extra": "^9.0.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"prettier": "^2.0.4",
		"sinon": "^9.0.2",
		"tslint": "^6.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"@types/deep-equal": "^1.0.1"
	},
	"engines": {
		"vscode": "^1.41.0"
	}
}